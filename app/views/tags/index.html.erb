<h1>All Tags</h1>
<table id="tags">
  <% @tags.each do |tag| %>
    <tr>
      <td><%= link_to tag.name, tag_path(tag), class: "tag" %></td>
      <% if current_user && current_user.email == "admin@example.com" %>
        <td><%= link_to "delete", tag_path(tag), method: :delete %></td>
      <% end %>
    </tr>
  <% end %>
</table>